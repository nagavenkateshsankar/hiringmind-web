{
  "hosting": {
    "public": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/services",
        "destination": "/services/index.html"
      },
      {
        "source": "/contact",
        "destination": "/contact/index.html"
      }
    ]
  }
}